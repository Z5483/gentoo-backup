CC="clang"
CXX="clang++"
COMMON_FLAGS="-flto=thin -O2 -pipe"
CFLAGS="${COMMON_FLAGS}"
CXXFLAGS="${COMMON_FLAGS} -stdlib=libc++"
LDFLAGS="-Wl,-O2 -Wl,--as-needed -Wl,--gc-sections -Wl,--strip-all -Wl,--strip-debug -march=native -mtune=native -O2 -flto=thin -fuse-ld=lld -rtlib=compiler-rt -unwindlib=libunwind"
AR="llvm-ar"
NM="llvm-nm"
RANLIB="llvm-ranlib"
