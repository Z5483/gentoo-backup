CC="clang"
CXX="clang++"
COMMON_FLAGS="-O2 -fno-lto -pipe"
CFLAGS="${COMMON_FLAGS}"
CXXFLAGS="${COMMON_FLAGS} -stdlib=libc++"
LDFLAGS="-Wl,-O2 -Wl,--as-needed -Wl,--gc-sections -Wl,--strip-all -Wl,--strip-debug -O2 -fno-lto -fuse-ld=lld -rtlib=compiler-rt -unwindlib=libunwind"
AR="llvm-ar"
NM="llvm-nm"
RANLIB="llvm-ranlib"
